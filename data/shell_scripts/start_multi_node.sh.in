#!/bin/bash
# TODO sending tasks to background makes it difficult to stop them and we miss
# the output...

${EQUALIZER_SERVER_BIN} ${EQUALIZER_CONFIG} &
./${PROGRAM_NAME} --eq-client --eq-listen ${HOSTNAME_TO_LISTEN}:${LISTENING_PORT} &
./${PROGRAM_NAME} -e ${DEMO_DATA_PATH}/env.env\
	-p ${DEMO_DATA_PATH}/${PROJ_PATH_RELATIVE}\
	-g ${DEMO_DATA_PATH}/global/global_config.prj\
	-v $@
