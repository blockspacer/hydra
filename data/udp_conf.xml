<server type="udp">
	<port>2244</port>

	<!-- Only one type of packets allowed -->
	<packet id="-1">
		<vector type="double" unit="mm">
			<map>arm1</map>
		</vector>
		<vector type="double" unit="mm">
			<map>arm2</map>
