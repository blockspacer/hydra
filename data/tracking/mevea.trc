<?xml version="1.0"?>

<clients>
	<!--
	incorrect_quaternion attribute uses (w, x, y, z) quaternion instead
	of VRPN (x, y, z, w) quaternion. It's recerved for dealing with buggy
	interfaces like Mevea's.
	-->
	<client type="VRPN" host="localhost" port="3884" incorrect_quaternion="true">
		<tracker name="DMTracker">
			<transformation>
				<quaternion w="0" x="-1" y="0" z="0"/> 
				<transformation>
					<quaternion w="0.7071" x="0" y="0.7071" z="0"/> 
				</transformation>
				<!--

				<transformation>
				<sign x="1" y="1" z="1"/>
					<transformation>
						<quaternion w="0.7071" z="0" y="0" x="-0.7071"/> 
					</transformation>
				<quaternion w="0.5" x="0.5" y="-0.5" z="-0.5"/>
				</transformation>
				-->
			</transformation>
			<!-- For some reason the first sensor is not in use 
			<sensor num="0"/>
			-->
			<sensor num="1">
				<trigger type="VRPN" name="Taka-akseliTrigger" />
			</sensor>
			<sensor num="2">
				<trigger type="VRPN" name="TakarunkoTrigger" />
			</sensor>
			<sensor num="3">
				<trigger type="VRPN" name="EturunkoTrigger" />
			</sensor>
			<sensor num="4">
				<trigger type="VRPN" name="NostovarsiTrigger" />
			</sensor>
			<sensor num="5">
				<trigger type="VRPN" name="KeinuvipuTrigger" />
			</sensor>
			<sensor num="6">
				<trigger type="VRPN" name="KoiranluuTrigger" />
			</sensor>
			<sensor num="7">
				<trigger type="VRPN" name="KauhaTrigger" />
			</sensor>
			<sensor num="8">
				<trigger type="VRPN" name="OERengasTrigger" />
			</sensor>
			<sensor num="9">
