<?xml version="1.0"?>

<env_config>

	<walls>
		<wall name="upper_left">
			<bottom_left x="-1.33" y="1.34" z="-1.33" />
			<bottom_right x="0" y="1.34" z="-1.33" />
			<top_left x="-1.33" y="2.34" z="-1.33" />
		</wall>
		<wall name="upper_right">
			<bottom_left x="0" y="1.34" z="-1.33" />
			<bottom_right x="1.33" y="1.34" z="-1.33" />
			<top_left x="0" y="2.34" z="-1.33" />
		</wall>
		<wall name="lower_left">
			<bottom_left x="-1.33" y="0.34" z="-1.33" />
			<bottom_right x="0" y="0.34" z="-1.33" />
			<top_left x="-1.33" y="1.34" z="-1.33" />
		</wall>
		<wall name="lower_right">
			<bottom_left x="0" y="0.34" z="-1.33" />
			<bottom_right x="1.33" y="0.34" z="-1.33" />
			<top_left x="0" y="1.34" z="-1.33" />
		</wall>
	</walls>

	<server port="4699" hostname=""/>

	<master name="master">
		<windows>
			<window name="window_upper_left" w="1280" h="1024" x="0" y="0">
				<channel name="upper_left">
					<wall>upper_left</wall>
				</channel>
			</window>
			<window name="window_upper_right" w="1280" h="1024" x="0" y="0">
				<channel name="upper_right">
					<wall>upper_right</wall>
				</channel>
			</window>
			<window name="window_lower_left" w="1280" h="1024" x="0" y="0">
				<channel name="lower_left">
					<wall>lower_left</wall>
				</channel>
			</window>
			<window name="window_lower_right" w="1280" h="1024" x="0" y="0">
				<channel name="lower_right">
					<wall>lower_right</wall>
				</channel>
			</window>
		</windows>
	</master>

</env_config>
