
import * from "normal_map.material"

material _missing_material_ 
{
	receive_shadows off
	technique
	{
		pass
		{
			ambient 0.1 0.1 0.1 1.0
			diffuse 0.8 0.0 0.0 1.0
			specular 0.5 0.5 0.5 1.0 12.5
			emissive 0.3 0.3 0.3 1.0
		}
	}
}

material ceiling : bling_phong/shadows
{
	set $ambientCol "0.737803 0.8 0.767903 1"
	set $diffuseCol "0.590243 0.64 0.614322 1"
	set $specularCol "0.5 0.5 0.5"
	set $shininess 50
}

material floor : bling_phong/shadows
{
	set $ambientCol "0.8 0.658905 0.264431 1"
	set $diffuseCol "0.64 0.527124 0.211545 1"
	set $specularCol "0.5 0.5 0.5"
	set $shininess 50
}

material walls : bling_phong/shadows
{
	//set $emissiveCol "0.0 0 0 0.2"
	set $ambientCol "0.8 0.693669 0.317046 1"
	set $diffuseCol "0.64 0.554935 0.253637 1"
	set $specularCol "0.5 0.5 0.5"
}

