// From Ogre examples
// Modified by Joonatan Kuosa to use shaders, for self shadowing

import * from "normal_map.material"

material ogre.mat/SOLID/TEX/ogre.tex.jpg/VertCol : bling_phong/shadows
{
	set $specularCol "0.0 0.0 0.0 1"
	set_texture_alias diffuse_map ogre_tex.png
}

material Material.001/SOLID/TEX/ogreahead.tex.jpg/VertCol : bling_phong/shadows
{
	set $specularCol "0.0 0.0 0.0 1"
	set_texture_alias diffuse_map ogreahead_tex.png
}

