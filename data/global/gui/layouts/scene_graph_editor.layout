<?xml version="1.0" ?>
<GUILayout>

<Window Type="DefaultGUISheet" Name="sceneGraphEditor">
	<Property Name="Text" Value="Scene Graph" />
	<Property Name="UnifiedMaxSize" Value="{{1,0},{1,0}}" />
	<Property Name="UnifiedPosition" Value="{{0.0,0},{0.0,0}}" />
	<Property Name="UnifiedSize" Value="{{1.0,0},{1.0,0}}" />
	<Property Name="Alpha" Value="0.4" />

	<Window Type="TaharezLook/Menubar" Name="sceneGraphEditor/menubar" >
		<Property Name="Text" Value="File" />
		<Property Name="UnifiedMaxSize" Value="{{1,0},{1,0}}" />
		<Property Name="UnifiedAreaRect" Value="{{0,0},{0,0},{1.0,0},{0.05,0}}" />
		<!-- Scene buttons -->
		<Window Type="TaharezLook/MenuItem" Name="editor/importSceneItem" >
			<Property Name="Text" Value="Import Scene" />
		</Window>
		<Window Type="TaharezLook/MenuItem" Name="editor/reloadScenes" >
			<Property Name="Text" Value="Reload Scenes" />
		</Window>
	</Window>

	<Window Type="TaharezLook/FrameWindow" Name="sceneGraphEditor/graph">
		<Property Name="UnifiedPosition" Value="{{0.0,0},{0.05,0}}" />
		<Property Name="UnifiedSize" Value="{{0.7,0},{0.90,0}}" />
		<Window Type="TaharezLook/Tree" Name="sceneGraphEditor/graph/Tree">
			<Property Name="UnifiedMaxSize" Value="{{1,0},{1,0}}" />
			<Property Name="UnifiedSize" Value="{{0.96,0},{0.90,0}}" />
			<Property Name="UnifiedPosition" Value="{{0.02,0},{0.02,0}}" />
		</Window>

		<Window Type="TaharezLook/Editbox" Name="sceneGraphEditor/graph/Editbox">
			<Property Name="VerticalAlignment" Value="Bottom" />
			<Property Name="UnifiedMinSize" Value="{{0.1,0},{0.04,0}}" />
			<Property Name="UnifiedSize" Value="{{0.96,0},{0.05,0}}" />
			<Property Name="UnifiedPosition" Value="{{0.02,0},{-0.02,0}}" />
			<Property Name="ReadOnly" Value="True" />
			<Property Name="Text" Value="" />
			<Property Name="Tooltip" Value="World Location X" />
		</Window>
	</Window>

	<Window Type="TaharezLook/MultiColumnList" Name="sceneGraphEditor/MultiColumnList" >
		<Property Name="UnifiedMaxSize" Value="{{1,0},{1,0}}" />
		<Property Name="UnifiedPosition" Value="{{0.7,0},{0.05,0}}" />
		<Property Name="UnifiedSize" Value="{{0.3,0},{0.95,0}}" />
		<Property Name="ColumnHeader" Value="text:Server Name width:{0.33,0} id:0" />
		<Property Name="ColumnHeader" Value="text:Address width:{0.5,0} id:1" />
		<Property Name="ColumnHeader" Value="text:Ping width:{0.2,0} id:2" />
	</Window>
