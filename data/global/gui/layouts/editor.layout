<?xml version="1.0" ?>
<GUILayout>
	<Window Type="DefaultWindow" Name="editor">

		<Window Type="TaharezLook/FrameWindow" Name="editor/Frame" >
			<Property Name="Text" Value="Editor tabs" />
			<Property Name="Tooltip" Value="Editor tabs." />
			<Property Name="TitlebarFont" Value="DejaVuSans-10" />
			<Property Name="UnifiedMaxSize" Value="{{1,0},{1,0}}" />
			<Property Name="UnifiedMinSize" Value="{{0,128},{0,128}}" />
			<Property Name="TitlebarEnabled" Value="True" />
			<Property Name="UnifiedAreaRect" Value="{{0.1,0},{0.1,0},{0.9,0},{0.9,0}}" />
			<Window Type="TaharezLook/TabControl" Name="editor/Frame/editor_tabs" >
			    <Property Name="Tooltip" Value="Editors" />
			    <Property Name="TabHeight" Value="{0,-1}" />
			    <Property Name="UnifiedMaxSize" Value="{{1,0},{1,0}}" />
			    <Property Name="TabPanePosition" Value="Bottom" />
			    <Property Name="UnifiedAreaRect" Value="{{0,10},{0,10},{1,-10},{1,-10}}" />
			</Window>
		</Window>

		<!-- Lower portion
		TODO custom tooltip needs to be set for this to work -->
		<Window Type="TaharezLook/Tooltip" Name="editor/tooltip" >
			<Property Name="Text" Value="Tooltip" />
			<Property Name="UnifiedMaxSize" Value="{{1,0},{1,0}}" />
			<Property Name="UnifiedAreaRect" Value="{{0,0},{0.95,0},{1,0},{0.05,0}}" />
		</Window>

	</Window>	<!-- Editor -->
</GUILayout>

